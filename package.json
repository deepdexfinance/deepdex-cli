{
	"name": "@deepdex/cli",
	"version": "1.1.0",
	"description": "High-performance CLI trading bot for the DeepDex protocol",
	"module": "index.ts",
	"type": "module",
	"bin": {
		"deepdex": "./index.ts"
	},
	"scripts": {
		"start": "bun index.ts",
		"dev": "bun --watch index.ts",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write --unsafe .",
		"format": "biome format ."
	},
	"private": false,
	"keywords": [
		"trading",
		"defi",
		"perpetuals",
		"spot",
		"cli",
		"bot"
	],
	"author": "DeepDex",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.3.7",
		"@types/bun": "latest",
		"@types/node": "^24.10.1"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.24.3",
		"bignumber.js": "^9.3.1",
		"consola": "^3.4.2",
		"dotenv": "^17.2.3",
		"viem": "^2.40.3",
		"zod": "^4.1.13"
	}
}